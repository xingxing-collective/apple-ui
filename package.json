{
  "name": "apple-ui",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/apple-ui.umd.js",
  "module": "./dist/apple-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/apple-ui.es.js",
      "require": "./dist/apple-ui.umd.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "pnpm -C playground dev",
    "build": "vite build",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "test": "vitest",
    "stub": "pnpm run -r --parallel stub",
    "prepare": "husky",
    "postinstall": "pnpm stub"
  },
  "dependencies": {
    "tailwind-variants": "^1.0.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "^9.22.0",
    "eslint-config-unjs": "^0.4.2",
    "husky": "^9.1.7",
    "tailwindcss": "^4.1.1",
    "typescript": "~5.7.2",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^3.0.8",
    "vue-tsc": "^1.8.27"
  },
  "packageManager": "pnpm@10.4.1"
}